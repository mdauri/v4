<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
     <rewrite>
        <rules>
            <clear />
            <rule name="API V4 AUTH LOGIN" stopProcessing="true">
                <match url="auth/token$" ignoreCase="true" />
                <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                <action type="Rewrite" url="login.php" appendQueryString="true" logRewrittenUrl="false" />
            </rule>
            <rule name="API V4 VRF LOGIN" stopProcessing="true">
                <match url="vrf/token$" ignoreCase="true" />
                <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                <action type="Rewrite" url="vrftoken.php" appendQueryString="true" logRewrittenUrl="false" />
            </rule>
            <rule name="API V4 CompanyGroups" stopProcessing="true">
                <match url="companygroups" ignoreCase="true" />
                <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                <action type="Rewrite" url="companygroups/companygroups.php?q={R1}" appendQueryString="true" logRewrittenUrl="false" />
            </rule>
            
            
        </rules>
        </rewrite>    
        <handlers>
            <remove name="php 56" />
            <remove name="PHP_via_FastCGI" />
            <add name="php 56" path="*.php" verb="*" modules="CgiModule" scriptProcessor="C:\Program Files (x86)\PHP\v5.6\php-cgi.exe" resourceType="Either" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
